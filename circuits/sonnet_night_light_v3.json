{
  "circuit": {
    "metadata": {
      "name": "Night Light with Photo Resistor",
      "description": "Yellow LED turns on when photo resistor detects low light. Photo resistor uses voltage divider with 10K resistor connected to analog input GP26. LED connects to GPIO GP15 with 220Î© current-limiting resistor.",
      "created": "2025-11-01"
    },
    "components": [
      {
        "id": "ldr1",
        "type": "photocell-ldr",
        "placement": {
          "pin0": "5F",
          "pin1": "8F"
        }
      },
      {
        "id": "r1",
        "type": "resistor-10k",
        "placement": {
          "pin0": "8E",
          "pin1": "11E"
        }
      },
      {
        "id": "led1",
        "type": "led-yellow-5mm",
        "placement": {
          "cathode": "20E",
          "anode": "21E"
        }
      },
      {
        "id": "r2",
        "type": "resistor-220",
        "placement": {
          "pin0": "21F",
          "pin1": "24F"
        }
      }
    ],
    "wires": [
      {
        "id": "w1",
