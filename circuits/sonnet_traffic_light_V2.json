          "pin0": "15D",
          "pin1": "20D"
        }
      },
      {
        "id": "led3",
        "type": "led-blue-5mm",
        "placement": {
          "anode": "25E",
          "cathode": "26E"
        }
      },
      {
        "id": "r3",
        "type": "resistor-220",
